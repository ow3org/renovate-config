{
  "description": "Sensible defaults for Renovate",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    ":prHourlyLimit2",
    ":prConcurrentLimit10",
    "group:monorepos",
    "group:recommended",
    "workarounds:all",
    "group:allNonMajor",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignoreDeps": [
    "unconfig"
  ],
  "meteor": {
    "enabled": false
  },
  "rangeStrategy": "bump",
  "npm": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  },
  "composer": {
    "commitMessageTopic": "{{prettyDepType}} {{depName}}"
  }
}
